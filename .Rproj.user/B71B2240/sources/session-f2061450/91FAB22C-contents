library(rtweet)
library(dplyr)
library(readr)
library(svDialogs)
library(tcltk)

# Prompt the user to enter a screen name
screen_name_env <- dlgInput("Enter a screen name: ")

# Extract the screen name from the environment
screen_name <- as.character(screen_name_env)

# Print the screen name to the console to confirm it was extracted correctly
print(screen_name)

